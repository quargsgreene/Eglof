#!/bin/sh
set -e
if test "$CONFIGURATION" = "Debug"; then :
  cd /Users/quargsgreene/code/Eglof/build/_deps/juce-build/tools
  make -f /Users/quargsgreene/code/Eglof/build/_deps/juce-build/tools/CMakeScripts/ReRunCMake.make
fi
if test "$CONFIGURATION" = "Release"; then :
  cd /Users/quargsgreene/code/Eglof/build/_deps/juce-build/tools
  make -f /Users/quargsgreene/code/Eglof/build/_deps/juce-build/tools/CMakeScripts/ReRunCMake.make
fi
if test "$CONFIGURATION" = "MinSizeRel"; then :
  cd /Users/quargsgreene/code/Eglof/build/_deps/juce-build/tools
  make -f /Users/quargsgreene/code/Eglof/build/_deps/juce-build/tools/CMakeScripts/ReRunCMake.make
fi
if test "$CONFIGURATION" = "RelWithDebInfo"; then :
  cd /Users/quargsgreene/code/Eglof/build/_deps/juce-build/tools
  make -f /Users/quargsgreene/code/Eglof/build/_deps/juce-build/tools/CMakeScripts/ReRunCMake.make
fi

